modules = ["python-3.11", "nodejs-20"]
[agent]
expertMode = true
integrations = ["discord:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["cmake"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Discord Bot"

[[workflows.workflow]]
name = "Discord Bot"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node src/index.js"
